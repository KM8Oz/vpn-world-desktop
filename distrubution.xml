<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="1">
    <title>vpnworld</title>
    <organization>dev.oldi.apps.vpn</organization>
    <domains enable_localSystem="true"/>
    <options customize="never" require-scripts="true" rootVolumeOnly="true"/>
    <background file="diagram.tiff" alignment="bottomleft" scaling="none"/>
    <welcome file="welcome.html" mime-type="text/html"/>
    <license file="license.txt"/>
    <pkg-ref id="dev.oldi.apps.vpn.pkg.client" version="0.1.0"
        auth="root">Build.pkg</pkg-ref>
    <choices-outline>
        <line choice="dev.oldi.apps.vpn.pkg.client"/>
    </choices-outline>
    <choice
        id="dev.oldi.apps.vpn.pkg.client"
        visible="true"
        title="vpnworld Client"
        description="vpnworld OpenVPN Client"
        start_selected="true">
      <pkg-ref id="dev.oldi.apps.vpn.pkg.client"/>
    </choice>
</installer-gui-script>